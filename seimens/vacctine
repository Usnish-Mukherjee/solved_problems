#include<bits/stdc++.h>
using namespace std;

int main()
{
int t;
cin>>t;
while(t--)
{
int n;
cin>>n;
int a[10];
for(int i=1;i<=9;i++)
cin>>a[i];
vector<string> v(n+1,"0");
for(int i=1;i<=n;i++)
{
for(int j=1;j<=9;j++)
{
if(a[j]<=i)
{
string x = v[i-a[j]];
string y = to_string(j);
string res;
if(x!="0")
res = to_string(max(stoi(x+y),stoi(y+x)));
else
res = y;
v[i] = to_string(max(stoi(v[i]),stoi(res)));
}
}
}

    if(v[n]=="0")
        cout<<-1<<endl;
    else
        cout<<v[n]<<endl;
}

}
