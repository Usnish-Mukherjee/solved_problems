#include <iostream>
using namespace std;

int main() {
	
	int n; cin >> n; 
	int key[n][2];
	for(int i=0; i<n; i++)
	{
	    cin >> key[i][0] >> key[i][1];
	}
	
	char ans = 'a' + key[0][0];
	int max = key[0][1];
	
	for(int i=1; i<n; i++)
	{
	    if(key[i][1] - key[i-1][1] > max)
	    {
	        max = key[i][1] - key[i-1][1];
	        ans = 'a' + key[i][0];   
	    }
	}
	
	cout << ans << endl;
	return 0;
}
